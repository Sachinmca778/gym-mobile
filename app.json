{
  "expo": {
    "name": "Gym CRM Mobile",
    "slug": "gym-crm-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#3B82F6"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.example.gymcrm"
    },
    "android": {
      "package": "com.example.gymcrm",
      "usesCleartextTraffic": true,
      "networkSecurityConfig": "xml/network_security_config"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [],
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}

